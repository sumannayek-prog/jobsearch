<!DOCTYPE html>
<html lang="en">
    <%-include("./common/header")%>

<body>
    <%-include("./common/navbar")%>

    <!-- Testimonial Start -->
    <div class="container-xxl py-5 wow fadeInUp" data-wow-delay="0.1s">
        <div class="container">
            <h1 class="text-center mb-5">Our Clients Say!!!</h1>
            <div class="owl-carousel testimonial-carousel">
                <% for(let item of blogdata) { if(item.status==1){%>
                <div class="testimonial-item bg-light rounded p-4">
                    <i class="fa fa-quote-left fa-2x text-primary mb-3"></i>
                    <p><%= item.aboutblog%></p>
                    <div class="d-flex align-items-center">
                        <img class="img-fluid flex-shrink-0 rounded" src="/<%= item.image%>" style="width: 50px; height: 50px;">
                        <div class="ps-3">
                            <h5 class="mb-1"><%= item.blogername%></h5>
                            <small>Profession</small>
                        </div>
                    </div>
                </div>
                <%}}%>
            </div>
        </div>
    </div>
    <!-- Testimonial End -->

    <%-include("./common/footer")%>
</body>
</html>

<!-- JavaScript Libraries -->
<script src="https://code.jquery.com/jquery-3.4.1.min.js"></script>
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0/dist/js/bootstrap.bundle.min.js"></script>
<script src="user/lib/wow/wow.min.js"></script>
<script src="user/lib/easing/easing.min.js"></script>
<script src="user/lib/waypoints/waypoints.min.js"></script>
<script src="user/lib/owlcarousel/owl.carousel.min.js"></script>

<!-- Template Javascript -->
<script src="user/js/main.js"></script>